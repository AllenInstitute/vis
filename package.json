		"buffer": "^5.5.0||^6.0.0",
		"parcel": "2.12.0",
		"prettier": "3.3.2",
		"process": "^0.11.10",
		"typescript": "^5.3.3"
	},
	"volta": {
		"node": "22.11.0",
		"pnpm": "9.14.2"
	}
